    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <comment text="A ile B aras&#305;nda C'ye tam b&#246;l&#252;nebilen say&#305;lar&#305;n toplan&#305;n&#305; bulunuz"/>
            <declare name="i, a, b, c, toplam" type="Integer" array="False" size=""/>
            <output expression="&quot;Ba&#351;lang&#305;&#231; de&#287;eri giriniz: &quot;" newline="True"/>
            <input variable="a"/>
            <output expression="&quot;Biti&#351; de&#287;eri giriniz: &quot;" newline="True"/>
            <input variable="b"/>
            <output expression="&quot;B&#246;l&#252;m giriniz: &quot;" newline="True"/>
            <input variable="c"/>
            <assign variable="toplam" expression="0"/>
