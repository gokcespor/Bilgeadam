                <if expression="i % c == 0">
                    <then>
                        <assign variable="toplam" expression="toplam + i"/>
                    </then>
                    <else/>
                </if>
            </for>
            <output expression="a &amp; &quot; ile &quot; &amp; b &amp; &quot; aras&#305;ndaki &quot; &amp; c &amp; &quot; ile tam b&#246;l&#252;nebilen say&#305;lar&#305;n toplam&#305;: &quot; &amp; toplam" newline="True"/>
        </body>
